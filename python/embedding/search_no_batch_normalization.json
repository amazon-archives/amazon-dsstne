{
    "Version" : 0.8,
    "Name" : "Search Winner 1.0",
    "Kind" : "FeedForward",  
    "ShuffleIndices" : false,
    "Layers" : [
        { "Name" : "Input_query", "Kind" : "Input", "N" : "auto", "DataSet" : "query", "Sparse" : true },
        { "Name" : "Hidden_query", "Kind" : "Hidden", "Type" : "FullyConnected", "Source" : "Input_query", "N" : 256, "Activation" : "tanh", "WeightInit" : { "Scheme" : "Gaussian", "Scale" : 0.02 , "Bias" : 0.0 }, "BatchNormalization" : false }, 
         
        { "Name" : "Input_asin", "Kind" : "Input", "N" : "auto", "DataSet" : "asin", "Sparse" : true },        
        { "Name" : "Hidden_asin", "Kind" : "Hidden", "Type" : "FullyConnected", "Source" : "Input_asin", "N" : 256, "Activation" : "tanh",
          "SharedWeights" : { "inputLayer" : "Input_asin", "SourceInputLayer" : "Input_query", "SourceOutputLayer" : "Hidden_query" }, "BatchNormalization" : false },
        
        { "Name" : "Output", "Kind" : "Output", "Type" : "Pooling", "Function" : "Cosine", "Source" : [ "Hidden_query", "Hidden_asin" ], "DataSet" : "target_share", "N" : "auto" }
    ],
        
    "ErrorFunction" : "L2Hinge"
}
